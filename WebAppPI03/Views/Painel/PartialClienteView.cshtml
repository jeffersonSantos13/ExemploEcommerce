@model WebAppPI03.Models.Cliente


<h4>Dados Cadastrais (Cliente)</h4>
<hr />



<div class="container">
    <form class="form-horizontal card col-md-10">

        <div class="form-group">
            <label class="control-label col-md-2">Nome: </label>
            <div class="col-md-4 ">
                <input type="text" class="form-control" disabled value="@Model.nomeCompletoCliente" />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Email: </label>
            <div class="col-md-4">
                <input type="text" class="form-control" disabled value="@Model.emailCliente" />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">CPF: </label>
            <div class="col-md-4">
                <input type="text" class="form-control" disabled value="@Model.CPFCliente" />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Data de nascimento: </label>
            <div class="col-md-4">

                <input type="text" class="form-control" disabled value="@Model.dtNascCliente" />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Celular: </label>
            <div class="col-md-4">

                <input type="text" class="form-control" disabled value="@Model.celularCliente" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">Tel. Comercial: </label>
            <div class="col-md-4">

                <input type="text" class="form-control" disabled value="@Model.telComercialCliente" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">Tel. Residencial: </label>
            <div class="col-md-4">
                <input type="text" class="form-control" disabled value="@Model.telResidencialCliente" />
            </div>
        </div>



    </form>
</div>

